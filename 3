#include <stdio.h>
int main ()
{
int a, b, add, subtract, multiply, divide;
a = 15;
b = 30;

add = a+b;
subtract = b-a;
multiply = a*b;
divide = b/a;
printf("the addition is %d", add);
printf("\nthe subtraction is %d", subtract);
printf("\nthe multiplication is %d", multiply);
printf("\nthe division is %d", divide);
return 0;
} 
